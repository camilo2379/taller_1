<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ControladorConsultaPersona</servlet-name>
        <servlet-class>controladores.ControladorConsultaPersona</servlet-class>        
    </servlet>
    <servlet>
        <servlet-name>ControladorConsultaVehiculo</servlet-name>
        <servlet-class>controladores.ControladorConsultaVehiculo</servlet-class>        
    </servlet>
    <servlet>
        <servlet-name>ControladorConsultaVenta</servlet-name>
        <servlet-class>controladores.ControladorConsultaVenta</servlet-class>        
    </servlet>
    <servlet>        
        <servlet-name>ControladorBienvenida</servlet-name>
        <servlet-class>controladores.ControladorBienvenida</servlet-class>        
    </servlet>
    <servlet>        
        <servlet-name>ControladorBienvenidaVehiculo</servlet-name>
        <servlet-class>controladores.ControladorBienvenidaVehiculo</servlet-class>        
    </servlet>  
    <servlet>        
        <servlet-name>ControladorBienvenidaVenta</servlet-name>
        <servlet-class>controladores.ControladorBienvenidaVenta</servlet-class>        
    </servlet>    
    <servlet-mapping>
        <servlet-name>ControladorBienvenida</servlet-name>
        <url-pattern>/ControladorBienvenida</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>ControladorBienvenidaVehiculo</servlet-name>
        <url-pattern>/ControladorBienvenidaVehiculo</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>ControladorConsultaVenta</servlet-name>
        <url-pattern>/ControladorConsultaVenta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorBienvenidaVenta</servlet-name>
        <url-pattern>/ControladorBienvenidaVenta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>        
        <servlet-name>ControladorConsultaPersona</servlet-name>
        <url-pattern>/ControladorConsultaPersona</url-pattern>
    </servlet-mapping>    
    <servlet-mapping>        
        <servlet-name>ControladorConsultaVehiculo</servlet-name>
        <url-pattern>/ControladorConsultaVehiculo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
